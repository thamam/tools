#!/bin/bash

set -e

echo "Removing Voice Transcription Tool..."

# Stop any running instances
pkill -f "voice-transcription-tool" || true
pkill -f "voice_transcription_tool" || true

# Clean up any temporary files
rm -rf /tmp/voice_transcription_* || true

echo "Voice Transcription Tool cleanup completed."

#DEBHELPER#

exit 0